<!DOCTYPE html>
<html>
  <head>
    <title>Music Share</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render 'layouts/navbar' %>
     <% flash.each do |type, message| %>
     <div class = "flash <%= type %>">
     <%= message %>
     </div>
     <% end %>
    <%= yield %>
  </body>
</html>
