
<%= form_for @song do |f| %>
<div class="container">

<div class="white-text">
  <h3><%= f.label :title %></h3>
  <h3><%= f.text_field :title %></h3>
</div>

<div class="white-text">
  <%= f.label :musician %>
  <%= f.text_field :musician %>
</div>

<div class="white-text">
  <%= f.label :comments %>
  <%= f.text_field :comments %>
</div>

<div class="white-text"><%= f.submit %></div>

</div>
<% end %>
